do{
CasoBase();
}while(!f.semanaPronta())
/*
De: Igor do Nascimento Alves
Data: 12/Outubro/2017
Objetivo: Interface da Classe Facu
*/


void CasoBase(){
	for(int i=0;i<f.getQuantProf();i++)
	if(f.getDiasDisp(i) == 1){
		f.setDia(f.indDisp[0],1);
		f.limpaDiasProf()
		//zerar professor e a disp semana
	}
}
