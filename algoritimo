do{
CasoBase();
}while(!f.semanaPronta())


void CasoBase(){
	for(int i=0;i<f.getQuantProf();i++)
		if(f.getDiasDisp(i) == 1){
			f.setDia(f.getIndDisp(i),i);
			f.limpaDiasProf(f.getIndDisp(i));
			f.menosDiaProf(i);
		}
}
